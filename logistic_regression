    {
      "cell_type": "code",
      "source": [
        "\n",
        "\n",
        "\n",
        "# Root Mean Squared Error (RMSE)\n",
        "\n",
        "def rmse(Y, Y_pred):\n",
        "    \"\"\"\n",
        "    Compute Root Mean Squared Error between actual and predicted values.\n",
        "\n",
        "    Parameters:\n",
        "    Y : ndarray\n",
        "        Actual target values\n",
        "    Y_pred : ndarray\n",
        "        Predicted target values\n",
        "\n",
        "    Returns:\n",
        "    rmse_val : float\n",
        "        Root mean squared error\n",
        "    \"\"\"\n",
        "    # Compute squared differences, take mean, then sqrt\n",
        "    rmse_val = np.sqrt(sum((Y - Y_pred) ** 2) / len(Y))\n",
        "    return rmse_val\n",
        "\n",
        "\n",
        "# Coefficient of Determination (R)\n",
        "\n",
        "def r2(Y, Y_pred):\n",
        "    \"\"\"\n",
        "    Compute R-squared metric to evaluate goodness-of-fit.\n",
        "\n",
        "    Parameters:\n",
        "    Y : ndarray\n",
        "        Actual target values\n",
        "    Y_pred : ndarray\n",
        "        Predicted target values\n",
        "\n",
        "    Returns:\n",
        "    r2_val : float\n",
        "        R-squared value (1 = perfect fit, 0 = predicts mean)\n",
        "    \"\"\"\n",
        "    mean_y = np.mean(Y)            # Mean of actual values\n",
        "    ss_tot = sum((Y - mean_y) ** 2)   # Total sum of squares\n",
        "    ss_res = sum((Y - Y_pred) ** 2)   # Residual sum of squares\n",
        "    r2_val = 1 - (ss_res / ss_tot)    # R formula\n",
        "    return r2_val\n"
      ],
      "metadata": {
        "id": "WTTfIrKpZ6fy"
      },
      "execution_count": 24,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Compute Root Mean Squared Error between actual and predicted prices\n",
        "print(\"RMSE:\", rmse(price, Y_pred))\n",
        "# Compute R-squared value to measure goodness-of-fit\n",
        "\n",
        "print(\"R2:\", r2(price, Y_pred))\n",
