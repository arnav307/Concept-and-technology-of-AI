    {
      "cell_type": "code",
      "source": [
        "# Create a column of ones for the bias (intercept) term\n",
        "# Length equals number of training examples\n",
        "x0 = np.ones(len(area_n))\n",
        "\n",
        "# Combine bias term and normalized features into a single design matrix\n",
        "# Transpose is used to get shape (m, 4) where:\n",
        "# m = number of samples\n",
        "# 4 = [1 (bias), area, age, floor]\n",
        "X2 = np.array([x0, area_n, age_n, floor_n]).T   # (m, 4)\n",
        "\n",
        "# Initialize weight vector (parameters) with zeros\n",
        "# One weight for each column in X2\n",
        "W = np.array([0, 0, 0, 0])\n",
        "\n",
        "# Store normalized house prices as the target output vector\n",
        "Y2 = np.array(price_n)\n",
        "\n",
        "# Print shapes to verify matrix dimensions before training\n",
        "print(\"X2 shape:\", X2.shape)   # Expected: (m, 4)\n",
        "print(\"Y2 shape:\", Y2.shape)   # Expected: (m,)\n",
        "print(\"W shape:\", W.shape)     # Expected: (4,)\n",
        "\n"
      ],
