    {
      "cell_type": "code",
      "source": [
        "def gradient_descent(X, Y, B, alpha, iterations):\n",
        "    \"\"\"\n",
        "    Perform gradient descent to optimize linear regression weights.\n",
        "\n",
        "    Parameters:\n",
        "    X : ndarray\n",
        "        Design matrix (m x n), including bias column\n",
        "    Y : ndarray\n",
        "        Target vector (m,)\n",
        "    B : ndarray\n",
        "        Initial weight vector (n,)\n",
        "    alpha : float\n",
        "        Learning rate\n",
        "    iterations : int\n",
        "        Number of iterations to run gradient descent\n",
        "\n",
        "    Returns:\n",
        "    B : ndarray\n",
        "        Optimized weight vector after gradient descent\n",
        "    cost_history : list\n",
        "        Cost at each iteration for monitoring convergence\n",
        "    \"\"\"\n",
        "\n",
        "    # Initialize list to store cost at each iteration\n",
        "    cost_history = [0] * iterations\n",
        "    m = len(Y)  # Number of training examples\n",
        "\n",
        "    # Loop over the number of iterations\n",
        "    for iteration in range(iterations):\n",
        "        # Compute predictions for current weights\n",
        "        Y_pred = X.dot(B)  # shape: (m,)\n",
        "\n",
        "        # Compute loss/error vector\n",
        "        loss = Y_pred - Y  # shape: (m,)\n",
        "\n",
        "        # Compute gradient (partial derivatives of cost w.r.t each weight)\n",
        "        dw = (X.T.dot(loss)) / m  # shape: (n,)\n",
        "\n",
        "        # Update weights using gradient descent rule\n",
        "        B = B - alpha * dw  # move in the direction of negative gradient\n",
        "\n",
        "        # Compute cost for monitoring convergence\n",
        "        cost = cost_function(X, Y, B)\n",
        "        cost_history[iteration] = cost\n",
        "\n",
        "    # Return optimized weights and cost history\n",
        "    return B, cost_history\n"
      ],
      "metadata": {
        "id": "b0rGO4QTZbzt"
      },
      "execution_count": 20,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "# Set hyperparameters for gradient descent\n",
        "\n",
        "\n",
        "alpha = 0.05       # Learning rate: controls step size in weight updates\n",
        "iterations = 8000  # Number of iterations to run gradient descent\n",
        "\n",
        "\n",
        "# Run gradient descent to optimize weights\n",
        "\n",
        "\n",
        "# new_weights -> optimized weight vector after training\n",
        "# cost_history -> list of cost values over all iterations\n",
        "new_weights, cost_history = gradient_descent(X2, Y2, W, alpha, iterations)\n",
        "\n",
        "# Print results\n",
        "\n",
        "\n",
        "print(\"New weights:\", new_weights)           # Optimized weights [bias, area, age, floor]\n",
        "print(\"Final cost:\", cost_history[-1])      # Cost after the last iteration\n"
      ],
