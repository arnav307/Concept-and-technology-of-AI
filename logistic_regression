      "cell_type": "code",
      "source": [
        "# Convert the 'HouseArea' column from the DataFrame into a NumPy array of floats\n",
        "# This will be a 1D array and is used as an input feature (X)\n",
        "area = df['HouseArea'].to_numpy(dtype=float)\n",
        "\n",
        "# Convert the 'HouseAge' column into a NumPy array of floats\n",
        "# Represents how old the house is (input feature)\n",
        "age = df['HouseAge'].to_numpy(dtype=float)\n",
        "\n",
        "# Convert the 'HouseFloor' column into a NumPy array of floats\n",
        "# Represents the number of floors in the house (input feature)\n",
        "floor = df['HouseFloor'].to_numpy(dtype=float)\n",
        "\n",
        "# Convert the 'HousePrice' column into a NumPy array of floats\n",
        "# This is the target/output variable (Y) for the model\n",
        "price = df['HousePrice'].to_numpy(dtype=float)\n"
      ],
      "metadata": {
        "id": "10VNnLMJXj1y"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "# Feature scaling (X variables)\n",
        "\n",
        "# Compute mean and standard deviation for HouseArea\n",
        "area_mean, area_std = area.mean(), area.std(ddof=0)\n",
        "\n",
        "# Compute mean and standard deviation for HouseAge\n",
        "age_mean, age_std = age.mean(), age.std(ddof=0)\n",
        "\n",
        "# Compute mean and standard deviation for HouseFloor\n",
        "floor_mean, floor_std = floor.mean(), floor.std(ddof=0)\n",
        "\n",
        "# Apply standardization (Z-score normalization) to features\n",
        "# Formula: (x - mean) / standard deviation\n",
        "area_n = (area - area_mean) / area_std\n",
        "age_n = (age - age_mean) / age_std\n",
        "floor_n = (floor - floor_mean) / floor_std\n",
        "\n",
        "\n",
        "\n",
        "# Target scaling (Y variable)\n",
        "\n",
        "\n",
        "# Compute mean and standard deviation for HousePrice (target variable)\n",
        "price_mean, price_std = price.mean(), price.std(ddof=0)\n",
        "\n",
        "# Standardize the target variable\n",
        "price_n = (price - price_mean) / price_std\n"
      ],
      "metadata": {
        "id": "OUgw6kOqYg8l"
      },
      "execution_count": 13,
      "outputs": []
